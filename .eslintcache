};\n\n  return (\n    <div\n      id=\"backdrop\"\n      onClick={(e) => { e.target === document.querySelector(\"#backdrop\") && dispatch(hideAppendModal()); }}\n    >\n      <div className=\"append-modal modal\">\n        <p className=\"modal__description\">Name your new color button:</p>\n        <input\n          name='colorName'\n          value={colorName}\n          onChange={e => setColorName(e.target.value)}\n          className=\"append-modal__input input-field\"\n          type=\"text\"\n          maxLength=\"16\"\n          placeholder=\"Enter name...\"\n          autoFocus\n        />\n        <br />\n        <div\n          className=\"cancel-btn modal-btn\"\n          onClick={() => dispatch(hideAppendModal())}\n        >\n          Cancel\n        </div>\n        <div\n          className=\"add-btn modal-btn\"\n          onClick={onSubmit}\n        >\n          Add!\n        </div>\n      </div>\n    </div >\n  )\n}\n\nexport default connect(null, { appendNewColor })\n  (AppendModal);\n","/Users/zedd/Documents/Projects/just-color/src/components/colorBtns/InitialColors.js",[],"/Users/zedd/Documents/Projects/just-color/src/components/layout/SettingsToggleBtn.js",[],"/Users/zedd/Documents/Projects/just-color/src/actions/colorActions.js",[],"/Users/zedd/Documents/Projects/just-color/src/reducers/colorReducer.js",[],"/Users/zedd/Documents/Projects/just-color/src/components/colorBtns/initialColors.js",[],"/Users/zedd/Documents/Projects/just-color/src/components/colorBtns/ColorBtns.js",[],{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":12,"column":10,"nodeType":"93","messageId":"94","endLine":12,"endColumn":13},"no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-unused-vars","'rgb' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]